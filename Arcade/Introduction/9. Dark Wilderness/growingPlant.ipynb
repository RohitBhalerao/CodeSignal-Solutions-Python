    "def growingPlant(upSpeed, downSpeed, desiredHeight):\n",
    "    height=0\n",
    "    count=0\n",
    "    \n",
    "    while(True):\n",
    "        count+=1\n",
    "        height+=upSpeed\n",
    "        if height >= desiredHeight:\n",
    "            return count\n",
    "        height-=downSpeed\n",
    "    \n"
   ]
