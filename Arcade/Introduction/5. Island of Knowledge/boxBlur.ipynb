    "def boxBlur(image):\n",
    "\n",
    "    r = len(image)\n",
    "    c = len(image[0])\n",
    "    ans = []\n",
    "    for i in range(1,r-1):\n",
    "        row=[]\n",
    "        for j in range(1,c-1):\n",
    "            row.append(sum([image[i+k][j+l] for k in [-1,0,1] for l in [-1,0,1]])//9)\n",
    "        ans.append(row)\n",
    "    return ans"
   ]
  }
