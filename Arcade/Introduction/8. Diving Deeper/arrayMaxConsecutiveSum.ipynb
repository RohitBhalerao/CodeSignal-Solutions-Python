    "def arrayMaxConsecutiveSum(inputArray, k):\n",
    "    \n",
    "    s = m = sum(inputArray[:k])\n",
    "    for i in range(k, len(inputArray)):\n",
    "        s += inputArray[i] - inputArray[i-k]\n",
    "        if s > m: m = s\n",
    "    return m"
   ]
  }
