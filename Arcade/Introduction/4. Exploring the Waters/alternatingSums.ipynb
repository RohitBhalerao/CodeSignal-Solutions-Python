    "def alternatingSums(a):\n",
    "        b = 0\n",
    "        c = 0\n",
    "        d = []\n",
    "        for i in range(len(a)):\n",
    "                if i%2==0:\n",
    "                        b = b + (a[i])\n",
    "                else:\n",
    "                        c = c + (a[i])\n",
    "        d.append(b)\n",
    "        d.append(c)\n",
    "        return d\n"
   ]
  }
