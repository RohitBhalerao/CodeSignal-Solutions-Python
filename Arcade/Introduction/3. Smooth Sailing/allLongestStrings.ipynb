    "def allLongestStrings(inputArray):\n",
    "\n",
    "    res = []\n",
    "    long = 0\n",
    "    for s in inputArray:\n",
    "        if len(s)==long:\n",
    "            res.append(s)\n",
    "        if len(s)>long:\n",
    "            long=len(s)\n",
    "            res = [s]\n",
    "        \n",
    "    return res"
   ]
  }
