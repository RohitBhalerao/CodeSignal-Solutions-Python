    "def differentSquares(matrix):\n",
    "    s = set()\n",
    "    for i in range(len(matrix) - 1):\n",
    "        for j in range(len(matrix[0]) - 1):\n",
    "            s.add((matrix[i][j], matrix[i][j+1], matrix[i+1][j], matrix[i+1][j+1]))\n",
    "    return len(s)\n",
    "    \n",
